function isq_related_product()
{
        var iploc = readCookie("iploc");
        var ipiso = getparamVal(iploc, "gcniso");
        var referrer = document.referrer;
        var city_type = "seller";
        city_id = $("#CITY_ID").val();
        if(referrer.match(/.google./g) || referrer.match(/\/impcat\//g))
        {    
            var imesh = readCookie("ImeshVisitor");
            var imesh_city = getparamVal(imesh,"ctid"); 
            if(imesh_city != '')
                {city_id = imesh_city;city_type = "buyer";}
            else
            {
                var ip_city = getparamVal(iploc,"gctid");
                if(ip_city != '')
                    {city_id = ip_city;city_type = "buyer";}
                else
                {
                    var geoLoc = readCookie("GeoLoc");
                    var geo_city = getparamVal(geoLoc,"lg_ctid");
                    if(geo_city != '')
                        {city_id = geo_city;city_type = "buyer";}
                }
            }
        }
        brd_mcat_id = $("#mcatid").val();
        glusr_id = $("#R_GLID").val();
        cat_id = $("#CATID").val();
        display_id = $("#displayid").val();
        adult_flag = $("#adult_flag").val();
        company_url = $("#company_url").val();
        pc_item_name =$("#pc_item_name").val();
        pcat_name = $("#pcat_name").val();
        pcat_flname =$("#pcat_flname").val();
        fob_currency =$("#fob_currency").val();
        R_COMPANY =$("#R_COMPANY").val();
        custtype =$("#custtype").val();
        R_CITY = $("#R_CITY").val();
        state_name = $("#state_name").val();
        country =$("#country").val();
        mobile = $("#cmpny_no").val();
        company_tscode = $("#company_tscode").val();
        supptype = $("#supptype").val();
        locality = $("#locality").val();
        mcat_fl = $("#mct_fl").val();
        mact_nm = $("#BRD_MCAT_NAME").val();
        var w_width = $(window).width();
        var webAddLoc = location.hostname;
        var pdp_env = webAddLoc.match(/^dev/) ? "dev-fcp" : (webAddLoc.match(/^stg/)) ? "stg-fcp" : "prod-fcp";
    
        if(brd_mcat_id != '')
        {
            $.ajax(
            {
                type: "GET",
                url: '/'+pdp_env+'/files/more_isq_related.php',
                data: {mcat_id:brd_mcat_id, gl_id:glusr_id, city_id: city_id, display_id: display_id, cat_id: cat_id, prod_serv:prod_serv, adult_flag:adult_flag, company_url:company_url, pc_item_name:pc_item_name, pcat_name:pcat_name, pcat_flname:pcat_flname, fob_currency:fob_currency, R_COMPANY:R_COMPANY, custtype:custtype, R_CITY:R_CITY, state_name:state_name, country:country, count_code:ipiso, contact_number:mobile, company_tscode:company_tscode, supptype:supptype, locality:locality, jsversion:128, pdp_js_ver:5, width:w_width,mcat_fl:mcat_fl,mact_nm:mact_nm,city_type:city_type,ad_type:pdp.ad_type},
                dataType: "text",
                success: function(response)
                {
                    if(response != '')
                    {
                        parsed_resp = JSON.parse(response);
                        $("#isq_data").html(parsed_resp['isq_html']);
                        $("#related_prod").html(parsed_resp['related_prod']);
                        $("#ex_mr").html(parsed_resp['more_prod']);
                        $("#scroll_product").val(parsed_resp['next_str']);
                        $("#isq_exclude").val(parsed_resp['exclude_item']);
                        $("#NEXT_ID").val(parsed_resp['next_id']);
                        $("#NEXT_url_"+display_id).val(parsed_resp['next_url']);
                        $("#NEXT_prd").html(parsed_resp['top_nxt_dtl']);
                        $(".ntxpdptop").html(parsed_resp['top_nxt_srt']);
                        $("#browse_related").html(parsed_resp['browse_related']);
                        scroll_product = (typeof $("#scroll_product").val() == 'undefined')?"":$("#scroll_product").val();//String for extended scroll
                        $("#next_click").html(parsed_resp['next_html']);
                        view_brc();//View more - view less functionality
                        pdpImgLoad();
                        var next_prdNm = parsed_resp['next_str'];
                        next_prdNm = next_prdNm.replace(/[0-9#]/g, '');
                        next_prdNm = next_prdNm.replace(/\+$/, '');
                        next_pn = next_prdNm.split("+");
                        next_pn_Ad = next_pn_Ad.concat(next_pn);
                        st= $('.sfirst').outerHeight()+$('#ex_mr').outerHeight();
                        pdHeight=$('#pdpD').outerHeight();
                        remFix=$('#fixreM').offset().top-200;
                        cmpSl=$('#aboutUs').offset().top-180;
                        vmd=$('#product_dtl').offset().top-115;
                        abtd=$('#abt').offset().top-280;
                        if(pdp.ad_type == 'AdSense')
                        {
                            if(document.getElementById("ca-pub-0673059417528889-4032348549") !== null)
                            {
                                show_ads_by_adsense();
                            }
                            if(document.getElementById("ca-pub-0673059417528889-7241460654") !== null){
                                show_ads_by_adsense();
                            }
                            
                            if(document.getElementById("google_ad_3") !== null || document.getElementById("google_ad_2") !== null || document.getElementById("google_ad_1") !== null){
                                show_ads_by_adsense();
                            }
                        }
                        else if(pdp.ad_type == "DFP")
                        {
                            show_ads_by_dfp('/3047175/PRODDTL_beforerelatedproducts_horizontaladunit', [[970, 90], [728, 90], [980, 120], [980, 90]], 'div-gpt-ad-1582184346604-0', pdp.blMcatID, pdp.blCatID, pdp.groupID);
                            show_ads_by_dfp('/3047175/PRODDTL_Firstfold_VerticalAdUnit', [[160, 600], [120, 600]], 'div-gpt-ad-1582184784863-0', pdp.blMcatID, pdp.blCatID, pdp.groupID);
                        }
                    }
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    console.log(XMLHttpRequest + textStatus + errorThrown);
                }
            });
        }
}
